{
  "configurations": [
    {
      "name": "stellar",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/build/src/",
        "${workspaceFolder}/build",
        "${workspaceFolder}/build/_deps/**/**/",
        "${workspaceFolder}/extern/**"
      ],
      "defines": [
        "GRPIC_ENGINE",
        "OUTPUT_ENABLED",
        "KERR_SCHILD_METRIC",
        "NTTINY_ENABLED"
      ],
      "cStandard": "c17",
      "cppStandard": "c++17",
      "forcedInclude": [
        "${workspaceFolder}/.vscode/intellisense.h"
      ],
      "configurationProvider": "ms-vscode.cmake-tools"
    },
    {
      "name": "wsl",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/build/**",
        "${workspaceFolder}/build/_deps/fmt-src/include/",
        "${workspaceFolder}/build/_deps/plog-src/include/",
        "${workspaceFolder}/build/_deps/kokkos-build/core/src",
        "${HOME}/Codes/kokkos/install/include",
        "${HOME}/Codes/adios2/install/include",
        "/usr/local/cuda/include",
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/build/src/",
        "${workspaceFolder}/extern/**"
      ],
      "defines": [
        // "PIC_ENGINE",
        "GRPIC_ENGINE",
        "SINGLE_PRECISION",
        "OMPENABLED",
        "OUTPUT_ENABLED",
        // "MINKOWSKI_METRIC",
        // "QSPHERICAL_METRIC",
        "QKERR_SCHILD_METRIC"
      ],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "forcedInclude": [
        "${workspaceFolder}/.vscode/intellisense.h"
      ],
      "configurationProvider": "ms-vscode.cmake-tools"
    }
  ],
  "version": 4
}